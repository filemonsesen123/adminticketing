<%- include ../layouts/header.ejs %>
 
<% if (messages.error) { %>
    <p style="color:red"><%- messages.error %></p>
<% } %>
 
<% if (messages.success) { %>
    <p style="color:green"><%- messages.success %></p>
<% } %>    
 
    <section class="content-header">
      <div class="row">
        <div class="col-xs-12">
          <div class="box">
            <div class="box-header with-border">
              <h3 class="box-title"><div class="box-title">Edit Admin</div></h3>
            </div>
            <div class="box-body">
            <form role="form" method="post" data-widget="collapse" data-toggle="tooltip" title="collapse" class="btn btn-box-tool"><i class="fa fa-minus"></i>
    <!--
        Using FOREACH LOOP for the users array
        
        myArray.forEach(function(el, index) {
            // el - current element, i - index
        });
    -->
    <% if (data) { %>
    <% data.forEach(function(user){ %>
                  <div class="row">
                  <div class="col-xs-6">
                  <div class="box-body">
                    <a href="/laporan/<%= user._id %>" class="text-center"><p><%= user.judul %></p></a>
                    <p><%= user.kategori %></p>
                    <p><%= user.ruang %></p>
                    <a href="/tolak/<%= user._id %>" class="text-center"><p>Tolak</p></a>
                  </div>
                  </div>
                  </div>
              <% }) %>
             <% } %>
             </form>
            </div>
          </div>
        </div>
      </div>
    </section>
  </div>
<!--
including footer content which is common for all pages
-->
<%- include ../layouts/footer.ejs %>