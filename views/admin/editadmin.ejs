  <script>
    $(document).ready(function(){
    var username,password,status,aid;
    $("#submit").click(function(){
        username=$("#username").val();
        password=$("#password").val();
        status=$("#status").val();
        aid=$("#aid").val();
        /*
        * Perform some validation here.
        */
            $.post("/editadm/"+aid,{username:username,password:password,status:status},function(data){
            if(data=== "done") {
                window.location.href="/admin";
            }
        });

    });
});
</script>
